<h1>Listing Addresstypes</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>

      <th></th>
    </tr>
  </thead>
  <tbody>
<%= for address_type <- @addresstypes do %>
    <tr>
      <td><%= address_type.code %></td>

      <td>
        <span><%= link "Show", to: Routes.address_type_path(@conn, :show, address_type) %></span>
        <span><%= link "Edit", to: Routes.address_type_path(@conn, :edit, address_type) %></span>
        <span><%= link "Delete", to: Routes.address_type_path(@conn, :delete, address_type), method: :delete, data: [confirm: "Are you sure?"] %></span>
      </td>
    </tr>
<% end %>
  </tbody>
</table>

<span><%= link "New Address type", to: Routes.address_type_path(@conn, :new) %></span>
