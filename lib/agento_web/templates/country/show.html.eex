<h1>Show Country</h1>

<ul>

  <li>
    <strong>Alpha2:</strong>
    <%= @country.alpha2 %>
  </li>

  <li>
    <strong>Alpha3:</strong>
    <%= @country.alpha3 %>
  </li>

  <li>
    <strong>Numeric3:</strong>
    <%= @country.numeric3 %>
  </li>

  <li>
    <strong>Name:</strong>
    <%= @country.name %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.country_path(@conn, :edit, @country) %></span>
<span><%= link "Back", to: Routes.country_path(@conn, :index) %></span>
